<div class="clock">
    <script>
        var inputa = $("#timeclock-a");
        inputa.clockpicker({
            autoclose: true
        });

        // Manual operations
        $('#button-a').click(function(e){
            // Have to stop propagation here
            e.stopPropagation();
            inputa.clockpicker('show')
                .clockpicker('toggleView', 'minutes');
        });
        $('#button-b').click(function(e){
            // Have to stop propagation here
            e.stopPropagation();
            inputa.clockpicker('show')
                .clockpicker('toggleView', 'hours');
        });

        //second
        var inputb = $("#timeclock-b");
        inputb.clockpicker({
            autoclose: true
        });

        // Manual operations
        $('#button-c').click(function(e){
            // Have to stop propagation here
            e.stopPropagation();
            inputb.clockpicker('show')
                .clockpicker('toggleView', 'minutes');
        });
        $('#button-d').click(function(e){
            // Have to stop propagation here
            e.stopPropagation();
            inputb.clockpicker('show')
                .clockpicker('toggleView', 'hours');
        });      

    </script>
</div>





