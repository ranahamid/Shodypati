@model IEnumerable<Shodypati.Models.Log>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<p>
    @Html.ActionLink("Delete All", "DeleteAllLog")
</p>

<div class="container">
    <div class="row">
        <div class="col-xs-12">
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>

                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ExceptionMessage)
                            </th>

                            @*<th>
            @Html.DisplayNameFor(model => model.ExceptionStackTrace)
            </th>*@

                            <th>
                                @Html.DisplayNameFor(model => model.ControllerName)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.IpAddress)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Browser)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.UserId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ActionName)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.CreatedOnUtc)
                            </th>

                        </tr>
                    </thead>
                    <tbody>
                   
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ExceptionMessage)
                        </td>
                        @*<td>
                            @Html.DisplayFor(modelItem => item.ExceptionStackTrace)
                        </td>*@
                        <td>
                            @Html.DisplayFor(modelItem => item.ControllerName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IpAddress)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Browser)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UserId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ActionName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreatedOnUtc)
                        </td>

                    </tr>
                    }
                    </tbody>
                </table>

            </div>
        </div>
    </div>
</div>
