@model List<Shodypati.Models.ApplicationUser>


@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}



@if (Model.Count == 0)
{
    <p>No Customer found!</p>
}

else
{
    <h4>List of Customer</h4>

    <table class="table">
        
        <thead>
            <tr>
                <th>User Name</th>
                <th>Email</th>
                <th>PhoneNumber</th>
                <th>PhoneNumber Confirmed</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                    <td>@item.PhoneNumber</td>
                    <td>@item.PhoneNumberConfirmed </td>                  
                </tr>
            }
        </tbody>
    </table>
}
